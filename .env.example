# Copy this file to .env and set your Google Gemini API key and proxy settings
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Optional: set a simple shared token to protect the proxy endpoint.
# Frontend should include this token in the header `x-app-token`.
PROXY_TOKEN=your_shared_proxy_token_here

# The origin allowed to call the proxy (for local dev change if needed)
FRONTEND_ORIGIN=http://localhost:3000
 
# Vite (frontend) needs a VITE_ prefixed variable to expose the token to the browser for local development.
# Copy PROXY_TOKEN value here as VITE_PROXY_TOKEN in your local .env (do NOT commit .env)
VITE_PROXY_TOKEN=your_shared_proxy_token_here
